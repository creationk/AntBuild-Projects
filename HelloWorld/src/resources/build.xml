<?xml version="1.0" encoding="UTF-8"?>
<project name="HelloWorld" default="info">
	<!-- <project name="HelloWorld" default="error"> -->
	<property file="build.properties"></property>
	<target name="info" depends="helloworld, smile">
		<echo>Hey, from Ant!!</echo>
		<echo>${ant.version}</echo>
		<echo>${basedir}</echo>
		<echo>${myMessage}</echo>
	</target>
	<target name="helloworld">
		<echo>Hello World!!</echo>
	</target>
	<target name="smile">
		<echo>:-)</echo>
	</target>
</project>